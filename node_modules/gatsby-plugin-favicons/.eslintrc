{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "node"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "rules": {
    "node/no-unsupported-features": 0
  }
}
